#include <stdio.h>
#include <conio.h>
#define SIZE 10
int ar[SIZE],top=-1;
void push(int value)
{
    if(top==10){
        printf("Ã­nvalid");
    }
        else{
            ar[top++]=value;
            printf("\n ar[%d] is %d",top,value);
        }
}
void pop()
{
    if(top==-1){
        printf("stack is empty");
    }
        else{
            printf("\n%d",ar[--top]);
        }
}
int main()
{
